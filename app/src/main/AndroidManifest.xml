<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.abc.smartattendance">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Smart Attendance"
        android:roundIcon="@drawable/empimg"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".EmployeesList" />
        <activity android:name=".AttendanceTaker" />
        <activity android:name=".PresentActivity" />
        <activity android:name=".AbsentActivity" />
        <activity android:name=".AttendanceTaker2" />
        <activity android:name=".PresentActivity2" />
        <activity android:name=".AbsentActivity2"></activity>
    </application>

</manifest>